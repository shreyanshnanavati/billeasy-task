# Application Configuration
NODE_ENV=development
PORT=3000

# File Upload Configuration (optional)
MAX_FILE_SIZE=5242880

FILE_PROCESSING_MIN_TIME=10000  # 10 seconds
FILE_PROCESSING_MAX_TIME=15000  # 15 seconds