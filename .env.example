# Database Configuration
DATABASE_URL="file:./prisma/dev.db"

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Redis Configuration (for BullMQ)
REDIS_HOST=localhost
REDIS_PORT=6379
